  <section id="eventslist">
    <button onclick="myF()" class="togglebtn">Event Toggler</button>
    <div class="row">
      <div class="ok card-deck border-dark" id="card1">
        <div class="card">
          <div class="card-header">
            <h3>Recent Events</h3>
          </div>
          <input type="text" id="searchInput">
            <button id="searchBtn">Search</button>
          <div class="card-body" style="height: 200px; overflow-y: scroll;">
            <h2></h2>
            
            <ul id="listoevent">
                <% eventList.forEach((event) => { %>
                    <li>
                        <%= event.eventName %>
                    </li>
                <% }) %>
            </ul>

          </div>
        </div>
      </div>

      <div class="ok card-deck border-dark" id="card2">
        <div class="card">
          <div class="card-header">
            <h3>Upcoming Events</h3>
          </div>
          <div class="card-body" style="height: 200px; overflow-y: scroll; background-color: #f5f9fa; filter: brightness(95%);">
            <h2></h2>
            <ul class="list-group" id="optionsList">
                <% eventList.forEach((event) => { %>
                    <li class="list-group-item" onclick="selectOption(this)">
                        <%= event.eventName %>
                    </li>
                <% }) %>
            </ul>
            

          </div>
          <div class="card-footer">
            <button class="btn btn-primary" id="selectBtn" disabled type="button">Sign Up</button>
            <% if (isUserAdmin === "true") { %>
                <button id="addEventBtn">+</button>
              <% } %>
          </div>
        </div>  
      </div>
      </div>

      <div class="ok2">
        
        <div class="card-list-container">
          <div class="card-list">
            <% for (var i=0; i < eventList.length; i++) { %>
                <div class = "cardd" data-event="<%= eventList[i].eventName %>">
                    <div class="card-front">
                        <h3>
                            <%= eventList[i].eventName %>
                        </h3>
                      </div>
                      <div class="card-back">
                        <p>
                            <%= eventList[i].eventDesc %>
                        </p>
                      </div>
                </div> 
            <% } %>
          </div>
        </div>
        
      </div>

      
    </div>
    
    

  </section>




